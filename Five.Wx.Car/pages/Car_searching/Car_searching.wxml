<!--pages/Car searching/Car searching.wxml-->
<view class="container">
  <view class="find-list" wx:for="{{carData}}" wx:key="carName" wx:for-item="items">
    <view class="find-items">
      <image src="/Img/显示图片/icon_find_car@2x.png" mode="scollFill center" class="find-icon"></image>
      <text class="series">{{items.carName}} {{items.carPrice}}万</text>
      <text class="find-type"></text>
    </view>
    <view class="find-items">
      <text class="font-type">{{items.carPin}}</text>
      <text class="font-type-1">{{items.carPai}} {{items.carAddr}}</text>
    </view>
    <view class="find-items">
      <view class="font-type">
        <text wx:if="{{items.channel == 1}}" class="channel"></text>
        <text>{{items.carColor}}</text>
      </view>
      <view class="font-type-1">
        <text>剩:{{items.carKu}}</text>
      </view>
    </view>
    <view class="find-items">
      <text wx:if="{{items.earnest_money > 0}}" class="earnest-money">己冻结诚意金{{items.earnest_money}}元</text>
    </view>
  </view>
</view>